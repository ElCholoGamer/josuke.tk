	"author": "ElCholoGamer",
	"license": "MIT",
	"private": true,
	"scripts": {
		"build": "yarn build:server && yarn build:app",
		"build:server": "tsc ./src/server/index.ts --resolveJsonModule --esModuleInterop --skipLibCheck --outDir ./src/server-prod/",
		"build:app": "webpack --config ./webpack/webpack.prod.js",
		"start": "webpack serve --config ./webpack/webpack.dev.js",
		"serve:dev": "ts-node-dev --transpile-only --rs --respawn ./src/server/index.ts -d",
		"serve": "node ./src/server-prod/index.js",
		"analyze": "cross-env ANALYZE=true yarn build:app",
		"lint": "eslint . --fix",
		"git": "yarn lint && git add ."
	},
